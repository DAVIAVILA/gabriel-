int main()
{

    int ano,meses,dias,idade;

    printf("informe quantos anos voce tem:");
    scanf("%d",&ano);

    printf("informe quantos meses voce tem:");
    scanf("%d",&meses);

    printf("informe quantos dias voce tem:");
    scanf("%d",&dias);

    idade = ano*365 + meses*30 + dias;



    retornar 0;
}
